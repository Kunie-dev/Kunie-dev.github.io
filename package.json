{
  "name": "kunie-portfolio-site",
  "version": "0.0.6",
  "private": true,
  "homepage": "https://portfolio.kunie.dev/",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lintfix": "prettier --write --list-different . && npm run lint:js -- --fix && npm run lint:style -- --fix",
    "prepare": "husky install",
    "release-alpha": "release-it --preRelease=alpha --ci",
    "release-beta": "release-it --preRelease=beta --ci",
    "release-rc": "release-it --preRelease=rc --ci",
    "release": "release-it --git.tagMatch='v[0-9]*\\\\.[0-9]*\\\\.[0-9]*-*' --ci",
    "release-minor-alpha": "release-it preminor --preRelease=alpha --ci",
    "release-minor-beta": "release-it preminor --preRelease=beta --ci",
    "release-minor-rc": "release-it preminor --preRelease=rc --ci",
    "release-major-alpha": "release-it premajor --preRelease=alpha --ci",
    "release-major-beta": "release-it premajor --preRelease=beta --ci",
    "release-major-rc": "release-it premajor --preRelease=rc --ci"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "prettier --check --ignore-unknown"
  },
  "dependencies": {
    "bootstrap": "^4.6.2",
    "bootstrap-vue": "^2.22.0",
    "core-js": "^3.25.3",
    "dayjs": "^1.11.5",
    "nuxt": "^2.15.8",
    "vue": "^2.7.10",
    "vue-server-renderer": "^2.7.10",
    "vue-template-compiler": "^2.7.10",
    "vue-typed-js": "^0.1.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "auto-changelog": "^2.4.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.5.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "release-it": "^15.5.0",
    "sass": "^1.55.0",
    "sass-loader": "^10.3.1",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^28.0.0"
  }
}
